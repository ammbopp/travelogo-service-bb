		"busboy": "^1.6.0",
		"connect-busboy": "^1.0.0",
		"copyfiles": "^2.4.1",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"exifreader": "^4.6.0",
		"express": "4.18.1",
		"helmet": "^7.0.0",
		"joi": "^17.7.0",
		"jsonwebtoken": "^8.5.1",
